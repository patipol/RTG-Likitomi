body {
    background: #FFFFFF;
    color: #4F5155;
    font-family: Lucida Grande, Verdana, Geneva, Sans-serif;
    font-size: 14px;	
	margin:0px;
}

table {
    border-collapse: collapse;
}

/*
 * Headbar
 */
#headbar-background {
    background: #F4F5F5 url(../images/gradient.png) repeat-x scroll left top;
    border-bottom: 1px solid #A5A5A5;
    height: 37px;
    position: fixed;
    right: 0px;
    top: 0px;
    width: 100%;
}

#topheader {
    background: transparent url(../images/header-bg.png) repeat-x scroll 0% 0%;
	height: 43px !important;
    position: fixed;
    top: 0px;
    width: 100%;
	z-index:9;
}

#headbar-short {
    position: fixed;
    font-size: 85%;
}

#headbar-short td {
	padding: 0px;
	vertical-align: middle;
	height: 37px !important;
	border-right:1px solid #A1A5A9;
	text-align:center;
	font-weight: bold;
}

#headbar {
    position: fixed;
    top: 0px;
    font-size: 85%;
    width: 100%;
	z-index:10;
}

#headbar td {
   	padding:0px 10px;
	vertical-align: middle;
	height: 43px !important;
}

