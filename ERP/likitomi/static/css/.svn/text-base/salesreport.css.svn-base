/* Links */
ul.primary-links li {
    float: left;
	cursor: pointer;
}

ul.primary-links li a, ul.primary-links li a:link, ul.primary-links li a:visited {
    color: #A0A0A0;
    background: transparent;
    text-decoration: none;
    padding: 14px 12px;
	cursor: pointer;
}

ul.primary-links li span {
    background: transparent url(../images/bg-navigation-item.png) no-repeat scroll 50% 100%;
    color: #DFE0E2;
    padding: 14px 12px;
	cursor: pointer;
}

ul.primary-links li a:hover{
	background: transparent url(../images/bg-navigation-item.png) no-repeat scroll 50% 100%;
    color: #FFFFFF;
    padding: 14px 12px;
	cursor: pointer;
}

ul.primary-links li a.selectedList
{
	background: transparent url(../images/bg-navigation-item.png) no-repeat scroll 50% 100%;
    color: #FFFFFF;
    padding: 14px 12px;
	cursor: pointer;
}

