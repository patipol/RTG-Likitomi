{% extends "CR/containerCR.html" %}
{% block view %}
<div id="wrapper">
    <div id="header"><a href="/likitomi/home/?eID={{eID}}">HOME</a> >> <a href="/likitomi/line/cr/end/?eID={{eID}}&pID={{pID}}">Finish CR</a><br>
    	<!-- start Menu-->
    <ul id="navlist">
		<li id="active"><a href="/likitomi">Logout</a></li>
	</ul>


	<!-- End Menu-->
	</div>
<!-- endheader-->
<!-- content-->
	<div id="browse_crag">
		<div class="login">
			{{title}}
		</div>
	</div>
<br>
<center>


<table cellspacing="2" cellpadding="2" class="tblFixedBorder" width="630">
	<tbody><tr>
        <td class="tblProdViewLabel">Sheet Code</td>
		<td class="tblProdViewLabel">Customer name</td>
		<td class="tblProdViewLabel">Product</td>
		<td class="tblProdViewLabel">Flute</td>
		<td class="tblProdViewLabel">DF</td>
		<td class="tblProdViewLabel">BM</td>
		<td class="tblProdViewLabel">BL</td>
		<td class="tblProdViewLabel">CM</td>
		<td class="tblProdViewLabel">CL</td>
		<td class="tblProdViewLabel">Length</td>
		<td class="tblProdViewLabel">Width</td>
		<td class="tblProdViewLabel">Case</td>
		<td class="tblProdViewLabel">Cut</td>
		<td class="tblProdViewLabel">Blank</td>
		<td class="tblProdViewLabel">Slit</td>
		<td class="tblProdViewLabel">Scoreline</td>
		<td class="tblProdViewLabel">Next Process</td>
	</tr>
	
<tr>
	<td class="tblProdView">{{product_code}}</td>
	<td class="tblProdView">{{cname}}</td>
	<td class="tblProdView">{{product_name}}</td>
	<td class="tblProdView">{{flute}}</td>
	<td class="tblProdView">{{df}}</td>
	<td class="tblProdView">{{bm}}</td>
	<td class="tblProdView">{{bl}}</td>
	<td class="tblProdView">{{cm}}</td>
	<td class="tblProdView">{{cl}}</td>
	<td class="tblProdView">{{length_mm}}</td>
	<td class="tblProdView">{{width_mm}}</td>
	<td class="tblProdView">{{case}}</td>
	<td class="tblProdView">{{cut}}</td>
	<td class="tblProdView">{{blank}}</td>
	<td class="tblProdView">{{slit}}</td>
	<td class="tblProdView">{{scoreline}}</td>
	<td class="tblProdView">{{next_process}}</td>
</tr>
</tbody></table>
<br><br>


<div id="boxcontainer" style="width:630px">
<table>
	<tbody><tr><form action="/likitomi/line/update/end/" method="get" name="startForm">
        <td class="tblDetailViewLabel">Finished at:</td>
		<td><input name="start_time" type="text"  value="{{current_date_time}}" style="text-align: center" disabled="disabled"/></td>
		<td class="tblDetailViewLabel">Amount of plan :</td>
		<td><input name="amount" type="text"  value='{{amount}}' style="text-align: center" /></td>
		<td> <input name="task" type="hidden" value="{{task}}">
			<input name="eID" type="hidden" value="{{eID}}">
			<input name="pID" type="hidden" value="{{pID}}">
			<input name="at" type="hidden" value="{{at}}"></td>
	</tr>
	<tr> 
	<td>
	</td>

	<td>
	</td>
	<td>
	</td>
	<td><br><input name="Enter" type="submit" value="Enter" /></td></tr>
	</tbody></table>
</div>

{% endblock %}
